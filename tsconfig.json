{
    "compilerOptions": {
        "module": "UMD",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "dist",
        "sourceMap": true,
        "allowJs": true,
        "noEmitOnError": true
    },
    "include": [
        "src/**/*"
    ]
}